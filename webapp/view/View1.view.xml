<mvc:View controllerName="cross.crossapp.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <VBox id="_IDGenVBox1">
                <ComboBox id="_IDGenComboBox1" selectionChange="onComboBoxSelectionChange"
                    items="{danModel>/uniqueArray}">
                    <items>
                        <core:Item id="_IDGenItem1" key="{danModel>CompanyName}" text="{danModel>CompanyName}" />
                    </items>
                </ComboBox>
            </VBox>
        </content>
    </Page>
</mvc:View>
